@inject NavigationManager Navigation

@*<MudButton OnClick="OnButtonClicked" Variant="Variant.Filled" Color="Color.Primary">@Label</MudButton>*@
<FluentButton OnClick="OnButtonClicked" Appearance="Appearance.Accent">
	@Label
</FluentButton>

@code {
	[Parameter]
	[EditorRequired]
	public string Page { get; set; } = string.Empty;

	[Parameter]
	[EditorRequired]
	public string Label { get; set; } = string.Empty;

	void OnButtonClicked() => Navigation.NavigateTo(Page);
}
